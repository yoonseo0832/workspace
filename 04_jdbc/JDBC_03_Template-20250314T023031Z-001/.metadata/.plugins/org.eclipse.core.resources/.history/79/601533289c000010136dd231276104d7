<comment>Properties Test</comment>

	<entry key="selectAll">
		SELECT *
		FROM MEMBER
		ORDER BY MEMBERNO
	</entry>
	<entry key="insertMember">
		INSERT INTO MEMBER(MEMBERNO, MEMBERID, MEMBERPW, ENROLLDATE) VAULES (SEQ_MNO.NEXTVAL, ?, ?, ?)
	</entry>
	<entry key="deleteMember">
		DELETE FROM MEMBER WHERER MEMBERID =?
	</entry>
	<entry key="updateMember">
		UPDATE MEMBER 
			SET MEMBERPW =?
		 WHERE MEMBERID =?
	</entry>
